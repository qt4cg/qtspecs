  NOTATION [shape="box", style="rounded", color="#e69f00", penwidth=4, label="xs:NOTATION" ]

  gDateTypes [shape="record", style="rounded", color="#e69f00", penwidth=4, label="{xs:gYear|xs:gMonth|xs:gDay|xs:gYearMonth|xs:gMonthDay}"];
