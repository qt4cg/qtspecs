  long -> int [dir=back ]
  int -> short [dir=back ]
  short -> byte [dir=back ]
  nonNegativeInteger -> {unsignedLong positiveInteger} [dir=back ]
  unsignedLong -> unsignedInt [dir=back ]
  unsignedInt -> unsignedShort [dir=back ]
  unsignedShort -> unsignedByte [dir=back ]

  string -> normalizedString [dir=back ]
